{
  "name": "obsidian-readlater",
  "version": "1.0.0",
  "description": "Sync read-later articles from multiple news sites into Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "read-later",
    "web-clipper"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "obsidian": "^1.4.16",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "playwright": "^1.40.1",
    "node-fetch": "^3.3.2"
  }
}
